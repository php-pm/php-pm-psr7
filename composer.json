{
    "name": "php-pm/psr7-adapter",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "description": "A PSR-7/PSR-15 middleware bridge for PHP-PM",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "php-pm/php-pm": "dev-react-0.8"
    },
    "autoload": {
        "psr-4": {
            "PHPPM\\Bridges\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andig/php-pm"
        }
    ]
}
